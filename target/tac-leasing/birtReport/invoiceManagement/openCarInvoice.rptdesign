            <method name="open"><![CDATA[importPackage(Packages.java.lang);
importPackage(Packages.java.util);
importPackage(Packages.com.brick.invoiceManagement.service);

var list=InvoiceManagementService.openInvoice(params["cardFlag"].value,params["financeDate"].value,params["companyCode"].value,params["caseType"].value,decodeURI(params["content"].value),"",params["employeeId"].value);

iter=list.iterator();]]></method>
            <method name="fetch"><![CDATA[if(iter.hasNext()==false){
	return false;
}
var data=iter.next();

row["LEASE_CODE"]=data.get("LEASE_CODE");
row["CUST_NAME"]=data.get("CUST_NAME");
row["PERIOD_NUM"]=data.get("PERIOD_NUM");
row["PAY_DATE"]=data.get("PAY_DATE");
row["PRICE"]=data.get("PRICE_WHITOUT_TAX");
row["DEPT_NAME"]=data.get("DEPT_NAME");
return true;]]></method>
