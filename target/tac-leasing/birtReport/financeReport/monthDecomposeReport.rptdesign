            <property name="dataSource">Data Source</property>
            <method name="open"><![CDATA[importPackage(Packages.java.lang);
importPackage(Packages.java.util);
importPackage(Packages.com.brick.report.service);

if(params["startDate"].value != null){
	
	var resultMap =InsuranceService.queryMonthDecomposeReportToExcel(params["startDate"].value,params["content"].value,params["companyCode"].value);
}

var list = resultMap.get("monthDecomposeReport");

iter=list.iterator();

vars["startDate"] = params["startDate"].value;
]]></method>
            <method name="fetch"><![CDATA[if(iter.hasNext()==false){
	return false;
}
var data=iter.next();
row["opposing_date"]=data.get("OPPOSING_DATE");
row["cust_name"]=data.get("CUST_NAME");
row["cust_code"]=data.get("CUST_CODE");
row["lease_code"]=data.get("LEASE_CODE");
row["ficb_item"]=data.get("FICB_ITEM");
row["real_price"]=data.get("REAL_PRICE");
row["bank_name"]=data.get("BANK_NAME");
row["operate_date"]=data.get("CREATE_TIME_OLD");

return true;
]]></method>
            <method name="close"><![CDATA[list=null;
data=null;
iter=null;]]></method>
